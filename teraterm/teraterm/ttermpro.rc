        MENUITEM SEPARATOR
        MENUITEM "&Macro",                      ID_CONTROL_MACRO
        MENUITEM "Show Macro &Window",          ID_CONTROL_SHOW_MACRO
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&Index",                      ID_HELP_INDEX2
        MENUITEM SEPARATOR
        MENUITEM "&About Tera Term...",         ID_HELP_ABOUT
    END
